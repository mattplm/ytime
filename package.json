{
  "name": "YTime",
  "version": "0.0.1",
  "description": "A small extension to display actual duration of a youtube video (taking playback rate into account)",
  "source": "src/app.ts",
  "main": "./dist/app.js",
  "targets": {
    "main": {
      "includeNodeModules": true,
      "optimize": true
    }
  },
  "scripts": {
    "build": "parcel build src/app.ts --experimental-scope-hoisting --no-source-maps --public-url ./",
    "watch": "parcel watch src/app.ts --public-url ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattplm/ytime.git"
  },
  "keywords": [
    "youtube"
  ],
  "author": "Matthias Paulmier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattplm/ytime/issues"
  },
  "homepage": "https://github.com/mattplm/ytime#readme",
  "devDependencies": {
    "@parcel/validator-typescript": "^2.9.3",
    "eslint": "^8.48.0",
    "parcel": "^2.9.3",
    "typescript": "^5.1.6",
    "web-ext": "^7.6.2",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-web-ext-tool": "^0.2.2",
    "webextension-polyfill-ts": "^0.26.0"
  }
}
